{
	"name": "stock-sentry",
	"version": "0.0.3",
	"description": "vscode stock sentry",
	"license": "MIT",
	"publisher": "EasonChiu",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"keywords": [
		"stock",
		"vscode"
	],
	"dependencies": {
		"@types/superagent": "^4.1.13",
		"axios": "^0.24.0",
		"superagent": "^6.1.0"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"eslint": "^8.2.0",
		"typescript": "^4.4.4"
	},
	"contributes": {
		"properties": {
			"stock-sentry.symbol": {
				"type": "string",
				"default": "sz000509",
				"description": "监控的股票代码"
			},
			"stock-sentry.bigVolume": {
				"type": "number",
				"default": 2000,
				"description": "大单监控，多少手以上的买卖单会特别提醒"
			},
			"stock-sentry.xueqiuCookie": {
				"type": "string",
				"default": "",
				"description": "雪球cookie，用于监控大单"
			},
			"stock-sentry.updateInterval": {
				"type": "number",
				"default": 2000,
				"description": "轮询间隔"
			},
			"stock-sentry.showTime": {
				"type": "array",
				"default": [
					"9:00",
					"15:30"
				],
				"description": "插件展示时间段"
			}
		}
	},
	"scripts": {
		"compile": "tsc -p ./",
		"build": "yarn lint && yarn compile && vsce package",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/easonchiu/stock-sentry"
	}
}
